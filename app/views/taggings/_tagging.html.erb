<li id="tagging-<%= tagging.id %>" class="tagging">
  <%= link_to tagging.name, search_index_path(:adv => 1, tags: tagging.name) %>
  <%= link_to '<i class="icon-remove-circle"></i>'.html_safe, tagging_path(tagging.name), method: :delete, remote: true, class: "delete-link", title: "delete this tag" %>
</li>
