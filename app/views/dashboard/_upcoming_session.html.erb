<div class="upcoming_session card card-body bg-light m-2" id='session-<%=session.id%>'>
  <span class="session"><%= link_to session.title, session %></span><br/>
  <span class="user"><%= session.user.name %></span><br/>
  <span class="start">Start:<%= session.start_datetime.to_s :short %></span><br/>
  <span class="People">People:<%= session.invitations.size %></span>
  <span class='tags'>Tags:<%=session.tag_list.to_s%></span><br/>
</div>
