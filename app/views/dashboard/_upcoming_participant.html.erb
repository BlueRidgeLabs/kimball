<div class="upcoming-participant well" id='person-<%=person.id%>'>
  <% 
    upcoming =  person.research_sessions.where('start_datetime between NOW() and ?', 1.week.from_now).last 
     invite = upcoming.invitations.find_by(person_id: person.id)
  %>
  <span class="name">
    <%= link_to "#{person.first_name} #{person.last_name}", person %>
  </span>
  <br/>
  <span class="session">
    Session:<%= link_to upcoming.title, upcoming %>
  </span>
  <br/>
  <span class="session-user">
    User: <%= upcoming.user.name %>
  </span>
  <br/> 
</div>
