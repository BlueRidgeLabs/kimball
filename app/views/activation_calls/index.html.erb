<p id="notice"><%= notice %></p>

<h1>Activation Calls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activation_calls.each do |activation_call| %>
      <tr>
        <td><%= link_to 'Show', activation_call %></td>
        <td><%= link_to 'Edit', edit_activation_call_path(activation_call) %></td>
        <td><%= link_to 'Destroy', activation_call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activation Call', new_activation_call_path %>
